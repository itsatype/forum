  <div class="demo-blog mdl-layout mdl-js-layout has-drawer is-upgraded">
    <main class="mdl-layout__content">
    	<div class="row">    
	      <div class="demo-blog__posts mdl-grid">
				  <% if flash[:notice] %>
				    <div class="mdl-layout-spacer"></div>
						<span class="mdl-chip mdl-chip--contact">
	                <span class="mdl-chip__contact mdl-color--white material-icons  material-icons md-36 mdl-color-text--blue">error</i></span>
						    <span class="mdl-chip__text"><%= flash[:notice] %></span>
						</span>
						<div class="mdl-layout-spacer"></div>  
				  <% end %>      
	      <div class="col-xs-12">			  
					<div class="mdl-card something-else mdl-cell mdl-cell--12-col">
						  <div class="mdl-card__media mdl-color--white mdl-color-text--grey-600">
						    <img src="https://ga-core.s3.amazonaws.com/cms/files/files/000/002/178/original/Thinx.png">
						  </div>
						  <div class="mdl-card__supporting-text meta meta--fill mdl-color-text--grey-600">
						    <div>
						      <strong>Thinx Forum</strong>
						    </div>
						<a href="<%= new_post_path %>">
						  <button class="mdl-button mdl-js-ripple-effect mdl-js-button mdl-button--fab mdl-color--accent" >
						    <i class="material-icons mdl-color-text--white" role="presentation">add</i>
							</button> 
						</a>				    
						  </div>
						</div>
					</div>
					<%= render 'shared/posts' %>
			</div>
		</div>
	</main>
</div>